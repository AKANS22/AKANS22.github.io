@using Ezeji_Udoka_Akans
@using Ezeji_Udoka_Akans.Models


@model List<EducationModel>

@{
    ViewData["Title"] = "Education";
}

<h2>Education</h2>

<table class="table">
    <thead>
        <tr>
            <th>SchoolName</th>
            <th>Certificate</th>
            <th>DateTime</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var education in Model)
        {
            <tr>
                <td>@education.SchoolName</td>
                <td>@education.Certificate</td>
                <td>@education.DateTime</td>
                <td>@education.Description</td>
            </tr>
        }
    </tbody>
</table>